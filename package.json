{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "Express API with Prisma and Supabase auth",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "node src/server.js",
    "start": "node src/server.js",
    "build": "prisma generate",
    "vercel-build": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "node --experimental-specifier-resolution=node prisma/seed.mjs",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
  "jose": "^5.8.0",
  "argon2": "^0.41.1",
  "jsonwebtoken": "^9.0.2",
  "express-rate-limit": "^7.3.1",
  "@supabase/supabase-js": "^2.45.4",
  "zod": "^3.23.8"
  },
  "devDependencies": {
  "jest": "^29.7.0",
  "prisma": "^5.17.0",
  "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/test/**/*.test.js"]
  },
  "prisma": {
    "seed": "node --experimental-specifier-resolution=node prisma/seed.mjs"
  }
}
